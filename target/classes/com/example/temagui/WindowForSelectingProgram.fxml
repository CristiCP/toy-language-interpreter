<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="800.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.temagui.WindowForSelectingProgramController">
   <children>
      <VBox alignment="CENTER" prefHeight="800.0" prefWidth="600.0" spacing="40.0">
         <children>
            <Label alignment="CENTER" text="Select a program:">
               <font>
                  <Font name="Arial Black" size="38.0" />
               </font>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Label>
            <ListView fx:id="listForPrograms" maxWidth="1.7976931348623157E308" prefHeight="774.0" prefWidth="600.0" />
            <Button fx:id="selectButton" mnemonicParsing="false" onAction="#selectButton" text="Select">
               <font>
                  <Font size="20.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
